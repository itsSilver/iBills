{
  "routes": [
    {
      "method": "GET",
      "path": "/categories",
      "handler": "category.find",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/categories/nested",
      "handler": "category.nested",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/categories/parents",
      "handler": "category.parents",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/categories/count",
      "handler": "category.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/categories/children/:id",
      "handler": "category.children",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/categories/:id",
      "handler": "category.findOne",
      "config": {
        "policies": []
      }
    },
    {
