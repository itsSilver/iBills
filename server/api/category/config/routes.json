    {
      "method": "POST",
      "path": "/categories/deleteAll",
      "handler": "category.deleteAll",
      "config": {
        "policies": ["warehouse.isAdmin"]
      }
    },
    {
      "method": "PUT",
      "path": "/categories/:id",
      "handler": "category.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/categories/:id",
      "handler": "category.delete",
      "config": {
        "policies": ["warehouse.isAdmin"]
      }
    }
  ]
